{
  extends: 'airbnb',
  env: {
    browser: true,
    node: true,
  },
  rules: {
      max-len: ['error', 160, 2,
      {
        ignoreUrls: true,
        ignoreComments: false,
        ignoreRegExpLiterals: true,
        ignoreStrings: true,
        ignoreTemplateLiterals: true,
      }
    ],
    "react/prefer-stateless-function": "off",
    "import/prefer-default-export": "off",
  }
}
